{
  "name": "nestjs-prisma",
  "version": "0.4.1",
  "scripts": {
    "build": "tsc",
    "check:type": "tsc --noEmit",
    "check:format": "prettier --check ./**/*.{js,json,ts,tsx}",
    "test": "ts-node ./tests/helpers/setup-tests.ts && jest --watch --verbose",
    "test:integration": "ts-node ./tests/helpers/setup-tests.ts && env-cmd jest --watch --verbose --config ./jest.config.integration.js",
    "test:ci": "ts-node ./tests/helpers/setup-tests.ts && jest --coverage --verbose --runInBand && jest --coverage --verbose --runInBand --config ./jest.config.integration.js",
    "format": "prettier --write ./**/*.{js,json,ts,tsx}"
  },
  "peerDependencies": {
    "@nestjs/graphql": ">=7",
    "@prisma/cli": "~2.3.0",
    "@prisma/client": "~2.3.0",
    "@types/graphql-fields": "^1.3.3",
    "@types/node": "*",
    "graphql-type-json": "^0.3.2"
  },
  "dependencies": {
    "@nestjs/graphql": "^7.6.0",
    "@prisma/generator-helper": "~2.3.0",
    "@prisma/sdk": "~2.3.0",
    "graphql-fields": "^2.0.3",
    "pluralize": "^8.0.0",
    "ts-morph": "^7.1.3",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@prisma/cli": "~2.3.0",
    "@prisma/client": "~2.3.0",
    "@types/graphql-fields": "^1.3.3",
    "@types/graphql-type-json": "^0.3.2",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.27",
    "@types/pg": "^7.14.4",
    "@types/pluralize": "0.0.29",
    "directory-tree": "^2.2.4",
    "env-cmd": "^10.1.0",
    "graphql": "^15.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-type-json": "^0.3.2",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "pg": "^8.3.0",
    "prettier": "^2.0.5",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "~26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "~3.9.7"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12.4"
  },
  "author": {
    "name": "Micha?ï¿½ Lytek",
    "url": "https://github.com/MichalLytek"
  },
  "contributors": [
    {
      "name": "wSedlacek",
      "url": "https://github.com/wSedlacek"
    },
    {
      "name": "EndyKaufman",
      "url": "https://github.com/EndyKaufman"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wSedlacek/nestjs-prisma.git"
  },
  "bugs": {
    "url": "https://github.com/wSedlacek/nestjs-prisma/issues"
  },
  "keywords": [
    "nestjs",
    "type-graphql",
    "typegraphql",
    "prisma",
    "prisma2",
    "prisma-framework",
    "graphql",
    "generator"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/generator.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write"
    ],
    "**/*.{js,json}": [
      "prettier --write"
    ]
  }
}
